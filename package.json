{
	"name": "@prismicio/mcp-server",
	"version": "0.0.17-alpha.5",
	"description": "Prismic Model Context Protocol (MCP) Server",
	"keywords": [
		"model context protocol",
		"mcp",
		"typescript",
		"prismic"
	],
	"repository": {
		"type": "git",
		"url": "ssh://git@github.com/prismicio/prismic-mcp-server.git"
	},
	"license": "Apache-2.0",
	"author": "Prismic <contact@prismic.io> (https://prismic.io)",
	"type": "module",
	"exports": {
		".": {
			"require": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.cjs"
			},
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		},
		"./stdio": {
			"require": {
				"types": "./dist/stdio.d.ts",
				"default": "./dist/stdio.cjs"
			},
			"import": {
				"types": "./dist/stdio.d.ts",
				"default": "./dist/stdio.js"
			}
		},
		"./package.json": "./package.json"
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"bin": {
		"stdio": "./bin/stdio.js"
	},
	"files": [
		"./dist",
		"./src"
	],
	"scripts": {
		"build": "vite build",
		"dev": "vite build --watch",
		"preview": "npx @modelcontextprotocol/inspector node bin/stdio",
		"format": "prettier --write .",
		"prepare": "npm run build",
		"lint": "eslint",
		"types": "vitest --typecheck.only --run",
		"types:watch": "vitest --typecheck.only",
		"unit": "vitest run --coverage",
		"unit:watch": "vitest watch",
		"size": "size-limit",
		"test": "npm run lint && npm run types && npm run unit && npm run build && npm run size"
	},
	"dependencies": {
		"@modelcontextprotocol/sdk": "^1.13.0",
		"zod": "^3.25.67"
	},
	"devDependencies": {
		"@eslint/js": "^9.29.0",
		"@size-limit/preset-small-lib": "^11.2.0",
		"@trivago/prettier-plugin-sort-imports": "^5.2.2",
		"@types/node": "^24.0.3",
		"@vitest/coverage-v8": "^3.2.4",
		"eslint": "^9.29.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-prettier": "^5.5.0",
		"eslint-plugin-tsdoc": "^0.4.0",
		"prettier": "^3.5.3",
		"prettier-plugin-jsdoc": "^1.3.2",
		"size-limit": "^11.2.0",
		"standard-version": "^9.5.0",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.34.1",
		"vite": "^6.3.5",
		"vite-plugin-sdk": "^0.1.3",
		"vitest": "^3.2.4"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
