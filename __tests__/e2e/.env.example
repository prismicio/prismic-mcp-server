# Copy this file to .env and fill in your API key to enable AI agent tests

# Bedrock AWS token for Claude integration
AWS_BEARER_TOKEN_BEDROCK=your_actual_aws_bearer_token_here
CLAUDE_CODE_USE_BEDROCK=1
AWS_REGION=us-east-1

# Optional: Enable Claude code debug mode for extra context
DEBUG_MODE=false

# Optional: Enable addition tool debugging
PRISMIC_DEBUG=false

# Optional: Prevent playwright for cleanup created projects in `playwright-tmp`
NO_CLEANUP=false
